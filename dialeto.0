dialeto = {
  dígito: {
    analisar: código => (
      código == "" ?
        {
          resto: ""
        } :
      código[0] >= 48 & código[0] <= 57 ?
        {
          valor: código[0] - 48
          resto: código[1:]
        } :
      {
        resto: código
      }
    )
    gerar: prefixo valor => `${prefixo}${valor + 48}`
  }
}

dialeto